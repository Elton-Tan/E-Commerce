<div class="row">
    {{> _admin-sidebar }} 
    <div class="col-sm-12 col-md-10">
        {{#if success}} 
				<div class="alert alert-success">{{msg}}</div> 
			{{/if}} 
			{{!-- Displays dive with {{error_msg}} if error_msg is valid --}} 
			{{#if fail}} 
				<div class="alert alert-danger">{{msg}}</div> 
			{{/if}}
        <table class="table">
            <thead>
                <tr>
                    <th scope="col" class="account-table-th">Ticket ID</th>
                    <th scope="col" class="account-table-th">Subject</th>
                    <th scope="col" class="account-table-th">Ticket Created At</th>
                </tr>
            </thead>
            <tbody>
                {{#each tickets_data}}
                <tr>
                    <td class="account-table-td">{{this.dataValues.ticket_no}}</td>
                    <td class="account-table-td"> <a href="/admin/contact-reply?tid={{this.dataValues.ticket_no}}">{{this.dataValues.subject}}</a> </td>
                    <td class="account-table-td">{{this.dataValues.createdAt}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>

<script>
      window.onload= function () {

		  
 document.getElementById('togglee').style.visibility = 'hidden';
	  }
	  
    
</script>